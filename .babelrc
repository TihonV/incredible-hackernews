{
  "presets": [
    [
      "env",
      {
        "targets": {
          "browsers": [
            "last 2 versions",
            "not android > 0",
            "not ios > 0",
            "not edge > 0",
            "not ie > 0",
            "not safari > 0"
          ]
        },
        "modules": false,
        "debug": false
      }
    ],
    "preact"
  ],
  "plugins": [
    ["transform-react-jsx", { "pragma": "h" }],
    "transform-class-properties",
    "transform-object-rest-spread"
  ]
}
